theme_minimal()
ggplot(faithful, aes(x = eruptions, y = ..density.., fill = as.factor(eruptions > 3))) +
geom_histogram(binwidth = 0.5, color = "black", alpha = 0.5) +
geom_density(alpha = 0.3, color = "blue") +  # Density overlay
facet_wrap(~ eruptions > 3, labeller = label_both) +  # Facet by short/long eruptions
scale_fill_manual(values = c("orange", "purple"), name = "Eruption Type") +
labs(title = "Histogram of Old Faithful Eruptions", x = "Eruption Duration (mins)", y = "Density") +
theme_bw()
